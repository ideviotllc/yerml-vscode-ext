{
    "name": "yerml",
    "displayName": "YERML",
    "description": "YAML-like Entity Relationship Modeling Language",
    "version": "0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "yerml",
            "aliases": ["YERML", "yerml"],
            "extensions": [".yerml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "yerml",
            "scopeName": "source.yerml",
            "path": "./syntaxes/yerml.tmLanguage.json"
        }]
    }
}